@model IList<UserInfoViewModel>

<h5 style="margin-bottom: 75px;font-size: 20px;font-weight:400;line-height:1.2">who has been ranked ?</h5>



<div class="container d-flex flex-column align-items-center">
    @* <div class="row d-flex justify-content-center"> *@
    @foreach (var user in @Model)
        {

            if (user.UserName.Equals("nothing"))
            {
                <p>has not been ranked yet by anybody</p>
            }
            else
            {
                <div class="row d-flex justify-content-around align-items-center" style="width: 500px;background-color: beige;margin-bottom: 5px;border-radius: 10px">
                    <img src="@user.UserImagePath" alt="User Photo" class="img-thumbnail" style="height: 100px;padding: 0">
                    <p>@user.UserName</p>
                    @* <p style="padding-right: 10px">8.1</p> *@
                </div>
            }

        }
 

    @* </div> *@
</div>